{
	"schemaVersion": 1,
	"id": "amblekit",
	"version": "${version}",
	"name": "AmbleKit",
	"description": "This is a library for fabric mods.",
	"authors": [
		"Amble Labs"
	],
	"contact": {
		"homepage": "https://amblelabs.github.io",
		"sources": "https://github.com/amblelabs/modkit/",
		"issues": "https://github.com/amblelabs/modkit/issues"
	},
	"license": "CC0-1.0",
	"icon": "assets/amblekit/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.amble.lib.AmbleKit"
		],
		"client": [
			"dev.amble.lib.client.AmbleKitClient"
		]
	},
	"mixins": [
		"amblekit.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"ait": "*"
	},
	"accessWidener": "amblekit.accesswidener"
}